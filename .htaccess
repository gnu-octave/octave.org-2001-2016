<FilesMatch "\.(in|m4)$">
  Order allow,deny
  Deny from all
</FilesMatch>

<FilesMatch "Makefile$">
  Order allow,deny
  Deny from all
</FilesMatch>

<FilesMatch "CVS">
  Order allow,deny
  Deny from all
</FilesMatch>

RewriteEngine on

RewriteRule ^(software/octave)/octave.pdf $1/doc/octave-4.0.1.pdf [L]

RewriteRule ^(software/octave/doc)/interpreter/(.*) $1/v4.0.1/$2 [L]
