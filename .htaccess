<FilesMatch "\.(in|m4)$">
  Order allow,deny
  Deny from all
</FilesMatch>

<FilesMatch "Makefile$">
  Order allow,deny
  Deny from all
</FilesMatch>

<FilesMatch "CVS">
  Order allow,deny
  Deny from all
</FilesMatch>

RewriteEngine on

RewriteRule ^octave.pdf http://www.gnu.org/software/octave/doc/octave-4.0.1.pdf [PT,L]

RewriteRule ^doc/interpreter/(.*) http://www.gnu.org/software/octave/doc/v4.0.1/$1 [PT,L]
